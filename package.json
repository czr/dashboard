{
  "name": "dashboard",
  "version": "1.0.0",
  "author": "Colin Z. Robertson",
  "description": "Personal dashboard",
  "repository": {
    "type": "git",
    "url": "https://github.com/czr/dashboard.git"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "serverdev": "cd server && npm run startdev",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "startdev": "concurrently --kill-others-on-fail \"npm run serverdev\" \"npm run client\"",
    "test": "npm run test-server && npm run test-time-tracking",
    "test-server": "cd server && npm test",
    "test-time-tracking": "cd time-tracking && npm test"
  },
  "dependencies": {
    "concurrently": "^4.1.0"
  },
  "devDependencies": {}
}
